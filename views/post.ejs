<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">
	<title>OrgPro</title>
</head>
<body>
	<div class= "container">
		<%- include("partials/navbar.ejs") %>
		<div class="jumbotron">
		<h1>Department</h1>
		
		<table class="table table-striped">
			
			<tbody>
				<% dep.forEach(function(deps){ %>
				<tr>
				
					<td><%- deps["name"] %></td>
				
					<td>
					<a href="/post/<%- deps['name'] %>" class="btn btn-primary">Show</a>
					</td>
				</tr>
				<% }) %>
			</tbody>
		</table>
		
	 </div></div>

			
 <%- include("partials/scripts.ejs") %>

</body>
</html>